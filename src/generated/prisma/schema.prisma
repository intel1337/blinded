generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String
  score    Int
}

model Song {
  id      Int    @id @default(autoincrement())
  name    String
  artist  String
  image   String
  genre   String
  preview String
}
